/* 헤더 버튼 */
.headerBtn li:hover{
    background: #012d4a;
    animation: btn 0.3s ease 0s 1 forwards;
    cursor: pointer;
}
.headerBtn li:nth-last-child(1):hover{
    background: #343434;
}

/* 정상  _nomal, 장애발생 _emer */

/* 맵 포인트 */
.Pemer{
    /* animation: pulse 0.7s linear 0s infinite normal; */
    background: #ff0000;
}
/* 알람 팝업 */
.pop_1{display: none;}
.pop_2{display: none;}
.pop_3{display: none;}
.pop_4{display: none;}
